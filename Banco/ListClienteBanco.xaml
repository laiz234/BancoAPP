        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Banco"
        mc:Ignorable="d"
        Title="Listar Clientes do Banco" Height="250" Width="400">
    <Grid>
        <ListBox x:Name="listBancos" Margin="23,21,31,132"/>
        <Button Content="Listar" HorizontalAlignment="Left" Margin="23,201,0,0" VerticalAlignment="Top" Click="ListarClick"/>
        <ListBox x:Name="listClientes" Margin="23,112,31,42" />
    </Grid>
</Window>
